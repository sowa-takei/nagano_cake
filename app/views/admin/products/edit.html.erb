<h1>商品編集</h1>
<%= form_with model: @product,url:edit_admin_product_path(@product), local:true do |f| %>
<label for="inputImage">商品画像</label>
<h4>商品名</h4>
 <%= f.text_field :name %>
<h4>商品説明</h4>
 <%= f.text_area :introduction %>
<h4>税抜価格</h4>
 <%= f.text_field :price %>円
<h4>販売ステータス</h4>
 <%= f.submit "変更を保存" %>
<% end %>